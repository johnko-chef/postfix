<% @settings.sort.reject{|k,v| k=='use_access_maps'}.reject{|k,v| k=='use_alias_maps'}.reject{|k,v| k=='use_transport_maps'}.reject{|k,v| k=='use_virtual_aliases'}.map do |key, value| -%>
<%  next if value.nil? -%>
<%  if value.kind_of? Array -%>
<%=   "#{key} = #{value.join(', ')}"%>
<%  else -%>
<%=   "#{key} = #{value}"%>
<%  end -%>
<% end -%>
